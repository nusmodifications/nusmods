{
  "name": "nusmods-export",
  "version": "0.0.1",
  "description": "Server-side timetable rendering for NUSMods export service",
  "main": "app.js",
  "author": "NUSModifications",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "forever start --spinSleepTime 2000 --killSignal=SIGTERM -c 'nodemon --exitcrash' ./app.js",
    "dev": "nodemon ./app.js",
    "devtools": "DEVTOOLS=1 nodemon ./app.js",
    "deploy": "rsync -avu --delete-after . ../../nusmods-export"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "bunyan": "^1.8.12",
    "forever": "^0.15.3",
    "fs-extra": "^5.0.0",
    "http-graceful-shutdown": "^2.1.0",
    "koa": "^2.5.0",
    "koa-router": "^7.4.0",
    "koa-views": "^6.1.3",
    "lodash": "^4.17.5",
    "nodemon": "^1.15.1",
    "pug": "^2.0.0-rc.4",
    "puppeteer": "^1.1.1",
    "raven": "^2.4.1"
  }
}
