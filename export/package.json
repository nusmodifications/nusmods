{
  "name": "nusmods-export",
  "version": "0.0.1",
  "description": "Server-side timetable rendering for NUSMods export service",
  "main": "src/index.js",
  "author": "NUSModifications",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "pm2 start ecosystem.config.js",
    "dev": "nodemon -r dotenv/config ./src",
    "devtools": "DEVTOOLS=1 nodemon -r dotenv/config ./src",
    "deploy": "rsync -avu --delete-after . ../../nusmods-export && pm2 restart ecosystem.config.js"
  },
  "dependencies": {
    "@sentry/node": "5.4.3",
    "axios": "0.19.0",
    "bunyan": "1.8.12",
    "fs-extra": "8.0.1",
    "http-graceful-shutdown": "2.3.2",
    "koa": "2.7.0",
    "koa-router": "7.4.0",
    "koa-views": "6.2.0",
    "lodash": "4.17.11",
    "nodemon": "1.19.1",
    "pug": "2.0.4",
    "puppeteer": "1.18.0"
  },
  "devDependencies": {
    "dotenv": "^8.0.0"
  }
}
