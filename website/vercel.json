{
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=2629800, immutable"
        }
      ]
    }
  ],
  "redirects": [
    { "source": "/api(/.*)", "destination": "https://api.nusmods.com$1" },
    { "source": "/news(/.*)", "destination": "https://blog.nusmods.com$1" }
  ]
}
