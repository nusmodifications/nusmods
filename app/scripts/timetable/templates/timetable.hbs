<div id="nm-timetable">
  <div class="panel">
    <div class="panel-body">
      <div class="shared-timetable-controls-region"></div>
      <div class="alert alert-danger hidden" id="clash">
        <strong>Warning!</strong>&nbsp;
        There are <strong class="clash">clashes</strong> in your exam timetable.
      </div>
      <div class="hide-title" id="timetable-wrapper"></div>
      <div class="timetable-controls">
        <div class="row">
          <div class="col-sm-6 col-md-7 col-lg-8">
            <div class="select-region"></div>
            <!-- <div class="alert alert-danger">
              So you think you don't have exams in the coming semester? Dream on! The
              exams data hasn't been published and will be reflected below when updated.
              Check back daily.
            </div> -->
            <div id="exam-timetable"></div>
            <div class="tips-region"></div>
          </div>
          <form role="form" class="col-sm-6 col-md-5 col-lg-4">
            <div class="form-group clearfix">
              <div class="export-region pull-left"></div>
              <div class="show-hide-region pull-right"></div>
            </div>
            <div class="form-group">
              <div class="url-sharing-region"></div>
            </div>
            <div class="form-group">
              <div class="semester-selector-region"></div>
            </div>
            <div class="hidden-xs">
              <div class="fb-page" data-href="https://www.facebook.com/nusmods/"
                data-tabs="timeline, messages" data-small-header="true" data-adapt-container-width="false"
                data-hide-cover="false" data-show-facepile="true">
                <blockquote cite="https://www.facebook.com/nusmods/" class="fb-xfbml-parse-ignore">
                  <a href="https://www.facebook.com/nusmods/">NUSMods</a>
                </blockquote>
              </div>
            </div>
          </form>
        </div>
      </div>
    </div>
  </div>
</div>
