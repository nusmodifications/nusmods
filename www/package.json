{
  "name": "nusmods",
  "version": "3.0.0-alpha",
  "description": "NUSMods v3",
  "scripts": {
    "start": "cross-env NODE_ENV=development node scripts/start",
    "start:export": "cross-env NODE_ENV=development CONFIG=timetable-only PORT=8081 OPEN_BROWSER=0 node scripts/start",
    "build": "cross-env NODE_ENV=production node scripts/build",
    "rsync": "rsync -avu dist/",
    "rsync:export": "rsync -avu --delete-after dist-timetable/",
    "promote-staging": "bash scripts/promote-staging.sh",
    "postinstall": "flow-scripts stub",
    "lint": "yarn flow && yarn lint:code && yarn lint:styles",
    "lint:code": "eslint --ext .js,.jsx src/js webpack scripts",
    "lint:styles": "stylelint \"src/**/*.scss\" --syntax scss",
    "size": "bundlesize",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "e2e": "nightwatch -e default,safariMobile,safariDesktop,edge",
    "cors": "node scripts/cors-crawler",
    "ci": "cross-env NODE_ENV=production npm run lint && npm test && npm rebuild node-sass && npm run build"
  },
  "author": "NUSModifications",
  "license": "MIT",
  "devDependencies": {
