{
  "name": "nusmods",
  "version": "3.0.0-alpha",
  "description": "NUSMods v3",
  "scripts": {
    "start": "cross-env NODE_ENV=development babel-node scripts/start",
    "build": "cross-env NODE_ENV=production babel-node scripts/build",
    "build:dll": "cross-env NODE_ENV=development webpack --config=webpack/webpack.config.dll.js",
    "postinstall": "npm run build:dll && flow-scripts stub",
    "flow": "flow check",
    "lint": "eslint_d test src/js/**/*.{js,jsx} webpack scripts",
    "lint:src": "eslint_d src/js/**/*.{js,jsx}",
    "lint:test": "eslint_d test",
    "lint:scripts": "eslint_d webpack scripts",
    "test": "jest --config jest.config.js",
    "test:watch": "jest --config jest.config.js --watch",
    "test:cov": "jest --config jest.config.js --coverage",
    "ci": "npm run lint && npm run build && npm run test:cov && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  },
  "author": "NUSModifications",
  "license": "MIT",
