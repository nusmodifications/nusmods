{
  "name": "nusmods",
  "version": "3.0.0-alpha",
  "description": "NUSMods v3",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --config=webpack/webpack.config.dev.js --port=9000 --open",
    "build": "cross-env NODE_ENV=production webpack --config=webpack/webpack.config.prod.js",
    "build:dll": "cross-env NODE_ENV=development webpack --config=webpack/webpack.config.dll.js",
    "postinstall": "npm run build:dll && flow-scripts stub",
    "flow": "flow check || exit 0",
    "lint": "npm run lint:src && npm run lint:test",
    "lint:src": "eslint 'src/js/**/*.{js,jsx}'",
    "lint:test": "eslint test",
    "test": "cross-env NODE_PATH=./src/js ava",
    "test:watch": "chokidar \"test/**/*.js\" -c \"npm test\" --initial",
    "ci": "npm run lint && npm run test",
    "coverage": "nyc npm test && nyc report --reporter=text-lcov | coveralls"
  },
  "author": "NUSModifications",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^6.7.7",
    "ava": "^0.16.0",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^6.2.5",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-syntax-object-rest-spread": "^6.13.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-register": "^6.24.1",
    "chokidar-cli": "^1.2.0",
    "clean-webpack-plugin": "^0.1.16",
    "coveralls": "^2.13.1",
    "cross-env": "^4.0.0",
    "css-loader": "^0.28.0",
    "enzyme": "^2.8.2",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-ava": "^4.2.0",
    "eslint-plugin-flowtype": "^2.32.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "flow-bin": "^0.45.0",
    "flow-scripts": "^0.2.0",
    "flow-status-webpack-plugin": "^0.1.6",
    "happypack": "^2.2.1",
    "html-webpack-plugin": "^2.28.0",
    "json-loader": "^0.5.4",
    "lodash-webpack-plugin": "^0.11.3",
    "nock": "^9.0.13",
    "node-sass": "^4.5.2",
    "nyc": "^8.3.0",
    "postcss-loader": "^1.3.3",
    "react-addons-test-utils": "^15.5.1",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.16.1",
    "stylelint-config-standard": "^16.0.0",
    "stylelint-webpack-plugin": "^0.7.0",
    "url-loader": "^0.5.8",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.1",
    "webpack-merge": "^4.1.0"
  },
  "dependencies": {
    "axios": "^0.14.0",
    "babel-polyfill": "^6.13.0",
    "bootstrap": "^4.0.0-alpha.4",
    "classnames": "^2.2.5",
    "d3": "^4.4.0",
    "dom-to-image": "^2.5.2",
    "ical-generator": "^0.2.9",
    "js-search": "^1.3.2",
    "lodash": "^4.15.0",
    "nusmoderator": "^2.0.0",
    "react": "^15.5.4",
    "react-addons-shallow-compare": "^15.5.2",
    "react-autobind": "^1.0.6",
    "react-disqus-thread": "^0.4.0",
    "react-document-title": "^2.0.3",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.4",
    "react-router": "^2.8.1",
    "react-router-redux": "^4.0.5",
    "react-select-fast-filter-options": "0.0.5",
    "react-virtualized-select": "^1.4.0",
    "redux": "^3.5.2",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.2.0"
  },
  "ava": {
    "babel": "inherit",
    "require": [
      "babel-register"
    ]
  },
  "nyc": {
    "all": true,
    "include": [
      "src/js"
    ],
    "exclude": [
      "src/js/types"
    ],
    "extension": [
      ".jsx"
    ],
    "require": [
      "babel-register"
    ]
  }
}
